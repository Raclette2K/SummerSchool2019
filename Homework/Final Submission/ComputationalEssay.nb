Cell[CellGroupData[{

Cell["Converting a Polynomial to Bits", "Section",
 CellChangeTimes->{{3.770648062640285*^9, 
  3.7706480704088573`*^9}},ExpressionUUID->"3d6eb08e-9f32-478f-9db9-\
1c425ff642f9"],

Cell[TextData[{
 "The polynomial form is useful for the computation, however we still want to \
transmit bits and not polynomials. For that, we can define another function \
that does what ",
 StyleBox["bitsToP", "Code"],
 " does, but in reverse. In addition to that, the function must also pad with \
0 in the case the maximum exponent of the polynomial is smaller than the \
number of bits we desire."
}], "Text",
 CellChangeTimes->{{3.770648072764776*^9, 3.770648103069421*^9}, {
  3.770648192334635*^9, 3.7706482124602737`*^9}, {3.7706591515483017`*^9, 
  3.770659237494459*^9}, {3.770659337098472*^9, 
  3.770659394694113*^9}},ExpressionUUID->"7e3dd9cd-2586-4e5c-8f9c-\
5c0f728cc9e5"],

Cell[BoxData[
 RowBox[{
  RowBox[{"pToBits", "[", 
   RowBox[{"polynomial__", ",", " ", "padding_Integer"}], "]"}], " ", ":=", 
  " ", 
  RowBox[{"Module", " ", "[", "\[IndentingNewLine]", "\t", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"expList", " ", "=", " ", 
      RowBox[{"Exponent", "[", 
       RowBox[{"polynomial", ",", "x", ",", "List"}], "]"}]}], "}"}], ",", 
    "\[IndentingNewLine]", "\t", 
    RowBox[{"Reverse", "[", "\[IndentingNewLine]", "\t\t", 
     RowBox[{"Table", "[", 
      RowBox[{
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{"MemberQ", "[", 
          RowBox[{"expList", ",", "i"}], "]"}], ",", "1", ",", "0"}], "]"}], 
       ",", " ", 
       RowBox[{"{", 
        RowBox[{"i", ",", "0", ",", 
         RowBox[{"padding", "-", "1"}]}], "}"}]}], "]"}], 
     "\[IndentingNewLine]", "\t", "]"}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Code",
 CellChangeTimes->{{3.7706592876750174`*^9, 3.7706592975720453`*^9}},
 CellLabel->"In[4]:=",ExpressionUUID->"9f426bb6-98df-4af1-ad0e-d9bf4a77511b"],

Cell["\<\
We also reverse the order in this function because we still use the MSB on \
the left convention.\
\>", "Text",
 CellChangeTimes->{{3.770659413350582*^9, 3.770659444656292*^9}, {
  3.770678488433609*^9, 
  3.770678488496422*^9}},ExpressionUUID->"0050872f-d5a4-463e-ad1f-\
faca8dd2ac3d"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"pToBits", "[", 
  RowBox[{
   FormBox[
    RowBox[{
     SuperscriptBox["x", "8"], "+", 
     SuperscriptBox["x", "5"], "+", 
     SuperscriptBox["x", "3"], "+", "1"}],
    TraditionalForm], ",", " ", "8"}], "]"}]], "Input",
 CellChangeTimes->{{3.770659307717314*^9, 3.770659334767948*^9}, {
  3.7707566526261053`*^9, 3.770756654255548*^9}, {3.7707569021476583`*^9, 
  3.770756926917199*^9}},
 CellLabel->"In[57]:=",ExpressionUUID->"57b3cb3e-f39a-4884-9c65-956e2480987d"],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{
  "0", ",", "0", ",", "1", ",", "0", ",", "1", ",", "0", ",", "0", ",", "1"}],
   "}"}]], "Output",
 CellChangeTimes->{
  3.770659397893079*^9, 3.770669152594039*^9, 3.7706781784207897`*^9, 
