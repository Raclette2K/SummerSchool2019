2a6db43f1dcd"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"strToP", "[", "str_String", "]"}], " ", ":=", " ", 
   "\[IndentingNewLine]", 
   RowBox[{"Module", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"bitList", " ", "=", " ", 
       RowBox[{"Reverse", "[", 
        RowBox[{"ImportString", "[", 
         RowBox[{"str", ",", "\"\<Bit\>\""}], "]"}], "]"}]}], "}"}], ",", " ", 
     RowBox[{"(*", " ", 
      RowBox[{
      "Reverse", " ", "because", " ", "the", " ", "MSB", " ", "is", " ", "on",
        " ", "the", " ", "left"}], " ", "*)"}], "\[IndentingNewLine]", 
     RowBox[{"bitList", ".", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"bitList", "[", 
          RowBox[{"[", "i", "]"}], "]"}], "*", 
         RowBox[{"x", "^", 
          RowBox[{"(", 
           RowBox[{"i", "-", "1"}], ")"}]}]}], ",", " ", 
        RowBox[{"{", 
         RowBox[{"i", ",", "1", ",", 
          RowBox[{"Length", "[", "bitList", "]"}]}], "}"}]}], "]"}]}]}], 
    RowBox[{"(*", " ", 
     RowBox[{"Dot", " ", "product", " ", "to", " ", "convert", " ", 
      RowBox[{"{", "}"}], " ", "in", " ", "a", " ", "polynomial"}], " ", 
     "*)"}], "\[IndentingNewLine]", "]"}]}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
